{
  "manifest_version": 2,
  "name": "Wisdom From Kanye",
  "version": "0.6",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": { "16": "icons/16.png",
    "48": "icons/48.png",
   "128": "icons/128.png" },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/48.png"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
}
}
